<div class="container" id="chat-messages">
	{{#each}}
	<div class="row">
		<div class="col-md-2 col-xs-4 col-md-offset-3 col-username">
			<span class="username">{{username}}</span> says:
		</div>
		<div class="col-md-2 col-xs-8">
			{{message}}
		</div>
		<hr></hr>
	</div>
	{{/each}}
</div>
<div class="container-fluid">
	<div class="row message-input-container">
		<input "col-md-12" placeholder="Type and press 'enter' to send"/>
	</div>
</div>
